# Config file for the Galois package
# It defines the following variables
#  Galois_INCLUDE_DIRS
#  Galois_LIBRARIES
#  Galois_LIBRARY_DIRS
#  Galois_BIN_DIRS
#  Galois_CXX_COMPILER
#  Galois_CXX_FLAGS

get_filename_component(GALOIS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(Galois_INCLUDE_DIRS "${GALOIS_CMAKE_DIR}/@PATH_FROM_INSTALLED_CMAKE_FILE_TO_PREFIX@@RELATIVE_INCLUDE_FROM_INSTALL_PREFIX@")

if(NOT TARGET galois_shmem AND NOT Galois_BIN_DIRS)
  include("${GALOIS_CMAKE_DIR}/GaloisTargets.cmake")
endif()

set(Galois_LIBRARIES galois_shmem)
set(Galois_LIBRARY_DIRS "${GALOIS_CMAKE_DIR}/@PATH_FROM_INSTALLED_CMAKE_FILE_TO_PREFIX@@RELATIVE_LIB_FROM_INSTALL_PREFIX@")
set(Galois_BIN_DIRS "${GALOIS_CMAKE_DIR}/@PATH_FROM_INSTALLED_CMAKE_FILE_TO_PREFIX@@RELATIVE_BIN_FROM_INSTALL_PREFIX@")
set(Galois_CXX_FLAGS "")
